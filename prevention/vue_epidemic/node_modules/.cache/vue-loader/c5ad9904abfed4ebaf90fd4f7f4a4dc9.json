{"remainingRequest":"D:\\1biyesheji\\code\\prevention\\vue_epidemic\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!D:\\1biyesheji\\code\\prevention\\vue_epidemic\\src\\components\\main\\user.vue?vue&type=style&index=0&id=4ebf9201&scoped=true&lang=scss&","dependencies":[{"path":"D:\\1biyesheji\\code\\prevention\\vue_epidemic\\src\\components\\main\\user.vue","mtime":1639843164275},{"path":"D:\\1biyesheji\\code\\prevention\\vue_epidemic\\node_modules\\css-loader\\dist\\cjs.js","mtime":1626773881347},{"path":"D:\\1biyesheji\\code\\prevention\\vue_epidemic\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":1639745719456},{"path":"D:\\1biyesheji\\code\\prevention\\vue_epidemic\\node_modules\\postcss-loader\\src\\index.js","mtime":1626773881844},{"path":"D:\\1biyesheji\\code\\prevention\\vue_epidemic\\node_modules\\sass-loader\\dist\\cjs.js","mtime":1628146127980},{"path":"D:\\1biyesheji\\code\\prevention\\vue_epidemic\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1626773863946},{"path":"D:\\1biyesheji\\code\\prevention\\vue_epidemic\\node_modules\\vue-loader\\lib\\index.js","mtime":1639745719456}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["user.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAkQA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA","file":"user.vue","sourceRoot":"src/components/main","sourcesContent":["<template>\r\n  <div>\r\n    <template>\r\n      <el-button style=\"display: none\" :plain=\"true\">打开消息提示</el-button>\r\n    </template>\r\n    <el-row>\r\n      <el-col :span=\"24\">\r\n        <el-card>\r\n          <div slot=\"header\" class=\"clearfix\" style=\"text-align: center\">\r\n            <span>个人信息</span>\r\n          </div>\r\n          <el-table :data=\"tableData\" border style=\"width: 100%\">\r\n            <el-table-column prop=\"username\" label=\"姓名\"> </el-table-column>\r\n            <el-table-column prop=\"classes\" label=\"班级\"> </el-table-column>\r\n            <el-table-column prop=\"sex\" label=\"性别\"> </el-table-column>\r\n            <el-table-column prop=\"address\" label=\"籍贯\"> </el-table-column>\r\n            <el-table-column label=\"入网时间\">\r\n              <template slot-scope=\"scope\">{{\r\n                scope.row.createtime | date\r\n              }}</template>\r\n            </el-table-column>\r\n            <el-table-column label=\"身份\">\r\n              <template slot-scope=\"scope\">{{\r\n                scope.row.type | type\r\n              }}</template>\r\n            </el-table-column>\r\n          </el-table>\r\n          <el-divider></el-divider>\r\n\r\n          <el-row :gutter=\"20\">\r\n            <el-col :span=\"12\">\r\n              <el-card\r\n                style=\"\r\n                  width: 80%;\r\n                  position: relative;\r\n                  left: 50%;\r\n                  transform: translateX(-50%);\r\n                \"\r\n              >\r\n                <div slot=\"header\" class=\"clearfix\" style=\"text-align: center\">\r\n                  <span>修改头像</span>\r\n                </div>\r\n                <el-upload\r\n                  class=\"avatar-uploader\"\r\n                  :action=\"iconUrl\"\r\n                  :show-file-list=\"false\"\r\n                  :on-success=\"handleAvatarSuccess\"\r\n                  :before-upload=\"beforeAvatarUpload\"\r\n                >\r\n                  <img v-if=\"imageUrl\" :src=\"imageUrl\" class=\"avatar\" />\r\n                  <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n                </el-upload>\r\n              </el-card>\r\n            </el-col>\r\n            <el-col :span=\"12\">\r\n              <el-card\r\n                style=\"\r\n                  width: 80%;\r\n                  position: relative;\r\n                  left: 50%;\r\n                  transform: translateX(-50%);\r\n                \"\r\n              >\r\n                <div slot=\"header\" class=\"clearfix\" style=\"text-align: center\">\r\n                  <span>修改密码</span>\r\n                </div>\r\n                <div style=\"text-align: center\">\r\n                  <el-input\r\n                    prefix-icon=\"el-icon-lock\"\r\n                    placeholder=\"请输入旧密码\"\r\n                    v-model=\"old_pwd\"\r\n                    show-password\r\n                    style=\"margin-top: 10px\"\r\n                  ></el-input>\r\n                  <el-input\r\n                    prefix-icon=\"el-icon-lock\"\r\n                    placeholder=\"请输入新密码\"\r\n                    v-model=\"new_pwd1\"\r\n                    show-password\r\n                    style=\"margin-top: 10px\"\r\n                  ></el-input>\r\n                  <el-input\r\n                    prefix-icon=\"el-icon-lock\"\r\n                    placeholder=\"请再输入密码\"\r\n                    v-model=\"new_pwd2\"\r\n                    show-password\r\n                    style=\"margin-top: 10px\"\r\n                  ></el-input>\r\n                  <el-button\r\n                    type=\"primary\"\r\n                    style=\"margin-top: 10px\"\r\n                    @click=\"repwd\"\r\n                    >提交修改</el-button\r\n                  >\r\n                </div>\r\n              </el-card>\r\n            </el-col>\r\n          </el-row>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      old_pwd: \"\",\r\n      new_pwd1: \"\",\r\n      new_pwd2: \"\",\r\n      tableData: [],\r\n      imageUrl: \"\",\r\n      imgPath: this.basePath + \"/file/\",\r\n      iconUrl: \"\",\r\n    };\r\n  },\r\n  created() {\r\n    let token = window.localStorage.token;\r\n    /**根据token获取当前用户信息\r\n     * */\r\n    this.$axiosjwt({\r\n      url: \"/users/getUserDataByToken\",\r\n      method: \"get\",\r\n      data: {},\r\n      success: (result) => {\r\n        this.tableData.push(result);\r\n      },\r\n      error: () => {\r\n        this.$router.push(\"/\");\r\n      },\r\n    });\r\n\r\n    /**\r\n     * 框架中的属性,修改头像并上传图片\r\n     * @type {string}\r\n     */\r\n    this.iconUrl = this.basePath + `/users/upicon?token=${token}`;\r\n  },\r\n  filters: {\r\n    date(time) {\r\n      var dt = new Date(time);\r\n      // yyyy-mm-dd\r\n      var y = dt.getFullYear();\r\n      var m = dt.getMonth() + 1;\r\n      var d = dt.getDate();\r\n      var hh = dt.getHours();\r\n      var mm = dt.getMinutes();\r\n      var ss = dt.getSeconds();\r\n      return y + \"-\" + m + \"-\" + d + \"  \" + hh + \":\" + mm + \":\" + ss;\r\n    },\r\n    type(i) {\r\n      if (i == 1) {\r\n        return \"管理员\";\r\n      }\r\n      if (i == 2) {\r\n        return \"学生\";\r\n      }\r\n      if (i == 3) {\r\n        return \"教师\";\r\n      }\r\n    },\r\n  },\r\n  methods: {\r\n    open(message, type) {\r\n      this.$message({\r\n        message: message,\r\n        type: type,\r\n      });\r\n    },\r\n\r\n    /**'\r\n     * 修改密码调用(需要补充格式验证)\r\n     */\r\n    repwd() {\r\n      let reg = /^(?=.*?[0-9])(?=.*?[a-z])[0-9a-z]{8,}$/;\r\n      if (this.new_pwd1 == this.new_pwd2) {\r\n        if (reg.test(this.new_pwd1)) {\r\n          this.$confirm(\"提交本次修改, 是否继续?\", \"提示\", {\r\n            confirmButtonText: \"确定\",\r\n            cancelButtonText: \"取消\",\r\n            type: \"warning\",\r\n          })\r\n            .then(() => {\r\n              this.upPwd();\r\n            })\r\n            .catch(() => {\r\n              this.$message({\r\n                type: \"info\",\r\n                message: \"已取消提交\",\r\n              });\r\n            });\r\n        } else this.open3(\"密码至少8位数,并且需要字母与数字混合\");\r\n      } else this.open3(\"输入的两次密码不一致\");\r\n    },\r\n    /**\r\n     * 修改密码函数\r\n     */\r\n    upPwd() {\r\n      this.$axiosjwt({\r\n        url: \"/users/upPwd\",\r\n        method: \"post\",\r\n        data: {\r\n          oldpassword: this.old_pwd,\r\n          newpassword: this.new_pwd1,\r\n        },\r\n        success: (result) => {\r\n          console.log(result);\r\n          if (result == \"密码修改成功\") {\r\n            this.open2(result);\r\n          } else {\r\n            this.open4(result);\r\n          }\r\n        },\r\n        error: () => {\r\n          this.$router.push(\"/\");\r\n        },\r\n      });\r\n    },\r\n\r\n    open2(v) {\r\n      this.$message({\r\n        message: v,\r\n        type: \"success\",\r\n      });\r\n    },\r\n\r\n    open3(v) {\r\n      this.$message({\r\n        message: v,\r\n        type: \"warning\",\r\n      });\r\n    },\r\n\r\n    open4(v) {\r\n      this.$message.error(v);\r\n    },\r\n    handleAvatarSuccess(res, file) {\r\n      this.open(\"头像修改成功,待下次登陆更换显示\", \"success\");\r\n      this.imageUrl = URL.createObjectURL(file.raw);\r\n    },\r\n    beforeAvatarUpload(file) {\r\n      const isJPG = file.type === \"image/jpeg\" || \"image/png\";\r\n      const isLt2M = file.size / 1024 / 1024 < 2;\r\n\r\n      if (!isJPG) {\r\n        this.$message.error(\"上传头像图片只能是 JPG、PNG 格式!\");\r\n      }\r\n      if (!isLt2M) {\r\n        this.$message.error(\"上传头像图片大小不能超过 2MB!\");\r\n      }\r\n      return isJPG && isLt2M;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n  position: relative;\r\n  left: 50%;\r\n  transform: translateX(-50%);\r\n  margin: auto;\r\n}\r\n\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409eff;\r\n}\r\n\r\n.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 178px;\r\n  height: 178px;\r\n  line-height: 178px;\r\n  text-align: center;\r\n}\r\n\r\n.avatar {\r\n  width: 178px;\r\n  height: 178px;\r\n  display: block;\r\n}\r\n</style>\r\n"]}]}